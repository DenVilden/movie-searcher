{
  "name": "movie-searcher",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "engines": {
    "node": "12.x",
    "yarn": "1.x"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "apollo-boost": "^0.4.4",
    "apollo-server-express": "^2.8.1",
    "axios": "0.18.1",
    "cors": "^2.8.5",
    "dayjs": "^1.8.15",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "jss": "10.0.0-alpha.23",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-apollo": "^3.0.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/node": "^7.5.5",
    "concurrently": "^4.1.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "nodemon": "^1.19.1",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "nodemon --exec babel-node server.js",
    "serve": "concurrently --kill-others-on-fail \"yarn start\" \"yarn client\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "rules": {
      "react/jsx-sort-props": 2,
      "import/no-extraneous-dependencies": "off"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
